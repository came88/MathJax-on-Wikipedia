# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Windows-style newlines with a newline ending every file, no blank spaces at the end of any line
[*]
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

# Set default charset as UTF-8
charset = utf-8

[*.js]
# Tab indent, width of tab = 4 spaces
indent_style = tab
indent_size = tab
tab_width = 4
